// Create a checkerboard using only ' ' and "* " 

#include<iostream>
using namespace std;

int main()
{
	int loop = 0;
	while (loop <= 3)
	{
	    
		cout << "* " << "* " << "* " << "* " << "* " << "* " << "* " << "* " << "* " << endl;
		cout << ' ' << "* " << "* " << "* " << "* " << "* " << "* " << "* " << "* " << "* " << endl;
		loop++;
	}
}
